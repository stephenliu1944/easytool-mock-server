{
  "name": "mock-serve",
  "version": "0.1.0-alpha.1",
  "description": "For front-end developers who need a quick back-end for Mocking data.",
  "license": "MIT",
  "author": "Stephen Liu",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenliu1944/mock-server"
  },
  "homepage": "https://github.com/stephenliu1944/mock-server",
  "bugs": {
    "url": "https://github.com/stephenliu1944/mock-server/issues"
  },
  "keywords": [
    "server",
    "fake",
    "API",
    "mock",
    "mocking",
    "mockjs",
    "test",
    "testing",
    "data"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "./lib/server/index.js",
  "bin": {
    "mock-serve": "./lib/cli/index.js"
  },
  "scripts": {
    "test": "node ./test/server/index.js",
    "package": "babel src -d lib",
    "lint:js": "eslint -c .eslintrc.prod.json --fix"
  },
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.1",
    "server-destroy": "^1.0.1",
    "shelljs": "^0.8.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^14.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js}": [
        "npm run lint:js",
        "git add"
      ],
      "ignore": []
    }
  }
}
